            "scenario_weights": {
              "browse_weight": 45,
              "search_weight": 20,
              "cart_weight": 20,
              "order_weight": 12,
              "profile_weight": 3
            },
            "service_load_characteristics": {
              "user_service_load": "medium",
              "inventory_service_load": "medium",
              "cart_service_load": "low",
              "order_service_load": "low",
              "payment_service_load": "low"
            }
          },
          {
            "name": "high_load_phase",
            "start_minute": 35,
            "duration_minutes": 25,
            "intensity": "high",
            "concurrent_users": 160,
            "user_behavior_distribution": {
              "quick_browsers": 0.3,
              "careful_shoppers": 0.25,
              "bulk_buyers": 0.2,
              "impulse_buyers": 0.25
            },
            "scenario_weights": {
              "browse_weight": 35,
              "search_weight": 15,
              "cart_weight": 30,
              "order_weight": 15,
              "profile_weight": 5
