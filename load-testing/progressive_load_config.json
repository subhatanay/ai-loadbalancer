{
  "rl_training_scenarios": {
    "description": "Progressive Load Configuration - Gradual Load Increase Testing",
    "version": "2.5",
    "created_for": "Progressive Load Testing to Find System Limits",
    
    "base_config": {
      "base_url": "http://localhost:8080/proxy",
      "request_timeout": 12,
      "retry_attempts": 2,
      "retry_delay": 0.8,
      "session_duration_minutes": 15,
      "think_time_min": 0.3,
      "think_time_max": 2.0,
      "connection_pool_size": 150,
      "max_retries_per_session": 3
    },

    "training_scenarios": {
      "progressive_load_test": {
        "description": "Progressive load increase - 2x baseline with gradual ramp",
        "total_users": 400,
        "test_duration_minutes": 100,
        "max_concurrent_users": 240,
        "ramp_up_time_seconds": 150,
        
        "traffic_patterns": [
          {
            "name": "warm_up_phase",
            "start_minute": 0,
            "duration_minutes": 15,
            "intensity": "low",
            "concurrent_users": 30,
            "user_behavior_distribution": {
              "quick_browsers": 0.5,
              "careful_shoppers": 0.3,
              "bulk_buyers": 0.1,
              "impulse_buyers": 0.1
            },
            "scenario_weights": {
              "browse_weight": 60,
              "search_weight": 25,
              "cart_weight": 10,
              "order_weight": 4,
              "profile_weight": 1
            },
            "service_load_characteristics": {
              "user_service_load": "low",
              "inventory_service_load": "low",
              "cart_service_load": "very_low",
              "order_service_load": "very_low",
              "payment_service_load": "very_low"
            }
          },
          {
            "name": "moderate_load_phase",
