            "service_load_characteristics": {
              "user_service_load": "high",
              "inventory_service_load": "high",
              "cart_service_load": "medium",
              "order_service_load": "medium",
              "payment_service_load": "medium"
            }
          },
          {
            "name": "peak_load_phase",
            "start_minute": 60,
            "duration_minutes": 20,
            "intensity": "very_high",
            "concurrent_users": 240,
            "user_behavior_distribution": {
              "quick_browsers": 0.2,
              "careful_shoppers": 0.2,
              "bulk_buyers": 0.3,
              "impulse_buyers": 0.3
            },
            "scenario_weights": {
              "browse_weight": 25,
              "search_weight": 10,
              "cart_weight": 40,
              "order_weight": 20,
              "profile_weight": 5
            },
            "service_load_characteristics": {
              "user_service_load": "very_high",
              "inventory_service_load": "very_high",
              "cart_service_load": "high",
              "order_service_load": "high",
              "payment_service_load": "high"
            }
          },
          {
            "name": "cool_down_phase",
            "start_minute": 80,
            "duration_minutes": 20,
            "intensity": "medium",
            "concurrent_users": 60,
            "user_behavior_distribution": {
              "quick_browsers": 0.6,
              "careful_shoppers": 0.3,
              "bulk_buyers": 0.05,
              "impulse_buyers": 0.05
            },
            "scenario_weights": {
              "browse_weight": 70,
              "search_weight": 20,
              "cart_weight": 7,
              "order_weight": 2,
              "profile_weight": 1
            },
            "service_load_characteristics": {
              "user_service_load": "medium",
              "inventory_service_load": "high",
              "cart_service_load": "low",
              "order_service_load": "very_low",
              "payment_service_load": "very_low"
            }
          }
        ]
      }
    },

